application HelloWorld {
	view:Tabs()
}

tabview Tabs {
	tab {
		title: "Devices"
		icon: "chat.png"
		view: Devices()
	}
}

tableview Devices {
	title: "iOS Devices"
	style: Grouped
	section {
		cell Default { text: "iPhone" }
		cell Default { text: "iPhone 3G" }
		cell Default { text: "iPhone 3GS" }
		cell Default { text: "iPhone 4" }
	}
	section {
		cell Default { text: "iPod Touch" }
		cell Default { text: "iPad" }
	}
}